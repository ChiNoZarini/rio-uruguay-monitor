# Configuración del Servidor
PORT=3000

# Configuración de Cache
CACHE_DURATION=43200000

# Configuración de Timeouts
REQUEST_TIMEOUT=15000

# Configuración de Actualización
UPDATE_INTERVAL=43200000
MIN_UPDATE_INTERVAL=120000

# Horarios de Actualización CARU (formato 24h, separados por coma)
CARU_UPDATE_HOURS=0,12

# Delay después de actualización CARU (en minutos)
UPDATE_DELAY_MINUTES=5

# Zona Horaria
TIMEZONE=America/Argentina/Buenos_Aires

# URL de la API de CARU
CARU_API_URL=http://190.0.152.194:8080/alturas/web/user/alturas

# User Agent para requests
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# Configuración de Desarrollo
NODE_ENV=production

# Configuración de Logs
LOG_LEVEL=info